<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="434.0" prefWidth="600.0" styleClass="anchor-pane" stylesheets="@../styles/medic.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.companies">
   <children>
      <VBox layoutY="139.0" prefHeight="263.0" prefWidth="159.0">
         <children>
            <Button mnemonicParsing="false" onAction="#insert_company" prefHeight="42.0" prefWidth="161.0" text="Insert a Company">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#update_company" prefHeight="42.0" prefWidth="158.0" text="Update a Company">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#CompanyDelete" prefHeight="42.0" prefWidth="161.0" text="Delete a Company">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin></Button>
            <Button fx:id="Button_Back" mnemonicParsing="false" onAction="#Button_Back" prefHeight="42.0" prefWidth="161.0" text="BACK">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
      <TableView fx:id="tableView" layoutX="172.0" layoutY="152.0" prefHeight="209.0" prefWidth="529.0">
         <columns>
            <TableColumn fx:id="CompanyName" prefWidth="111.0" text="CompanyName" />
            <TableColumn fx:id="CompanyCountry" prefWidth="91.0" text="Country" />
            <TableColumn fx:id="CompanyEmail" prefWidth="120.0" text="Company Email" />
            <TableColumn fx:id="CompanyContact" prefWidth="92.0" text="Contact" />
            <TableColumn fx:id="Address" prefWidth="155.0" text="Address" />
         </columns>
      </TableView>
      <ImageView fitHeight="132.0" fitWidth="186.0" layoutY="-11.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/logop.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
